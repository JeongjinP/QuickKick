<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC"-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.springboot_back.mapper.MemberMapper">
    <select id="findAll" resultType="com.springboot.springboot_back.domain.Member">
        SELECT * FROM login
    </select>
    <select id="findByID" parameterType="int" resultType="com.springboot.springboot_back.domain.Member">
        SELECT * FROM login WHERE id = #{id}
    </select>
</mapper>